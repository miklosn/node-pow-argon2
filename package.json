{
  "name": "node-pow-argon2",
  "version": "0.1.4",
  "description": "An Argon2-based proof of work library for Node",
  "keywords": [
    "pow",
    "argon2"
  ],
  "homepage": "https://github.com/miklosn/node-pow-argon2",
  "repository": {
    "type": "git",
    "url": "git@github.com:miklosn/node-pow-argon2.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "jest lib/",
    "build": "babel lib/ --presets babel-preset-es2015 --out-dir dist/",
    "prepublish": "npm run build"
  },
  "author": "Miklos Niedermayer",
  "license": "Unlicense",
  "dependencies": {
    "argon2": "^0.17.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.2",
    "regenerator-runtime": "^0.12.0",
    "sandra": "^0.2.1"
  },
  "babel": {
    "sourceMaps": "both",
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
